f = open(’1_26_18_04.txt’)
s, n = map(int, f.readline().split())
l = [int(i) for i in f]
l.sort()
sm = 0
cnt = 0
mx = 0
for i in range(n):
    if sm + l[i] <= s:
        sm += l[i]
        mx = l[i]
        cnt += 1
    else:
        break

if sm < s:
    sm -= mx
    for j in range(n):
        if sm + l[j] <= s:
            mx = max(mx, l[j])
        else:
            break

print(cnt, mx)

f = open("26.txt")
n=int(f.readline())
array=[int(n) for n in f.readlines()]
array.sort()
counter = 0
for i in range(0, n - 1):
    for j in range(n - 1, i, -1):
        if (array[i] + array[j] == 100 and array[i] != 0 and array[j] != 0):
            counter += 1
            array[j] = 0
            break
print(counter)
